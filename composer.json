{
    "name": "camelot/coding-style",
    "description": "Coding style for the Camelot project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        },
        {
            "name": "Carson Full",
            "email": "carsonfull@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.12",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Camelot\\CsFixer\\": "src",
            "Camelot\\Sniffs\\": "Camelot/Sniffs"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camelot\\Tests\\": "Camelot/Tests"
        }
    }
}
