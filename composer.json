{
    "name": "camelot/coding-style",
    "description": "Coding style for the Camelot project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        },
        {
            "name": "Carson Full",
            "email": "carsonfull@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.9",
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0",
        "phpunit/phpunit": "^7.5 || ^8.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Camelot\\CsFixer\\": "src",
            "Camelot\\Sniffs\\": "Camelot/Sniffs"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camelot\\Tests\\": "Camelot/Tests"
        }
    }
}
